<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>CARO</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- owl carousel style -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.0.0-beta.2.4/assets/owl.carousel.min.css" />
    <!-- bootstrap css -->
    <link rel="stylesheet" type="text/css" href="../../Content/styles/bootstrap.min.css">
    <!-- style css -->
    <link rel="stylesheet" type="text/css" href="../../Content/styles/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="../../Content/styles/responsive.css">
    <!-- fevicon -->
    <link rel="icon" href="../../Content/images/logo.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="../../Content/styles/jquery.mCustomScrollbar.min.css">
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <!-- owl stylesheets --> 
    <link rel="stylesheet" href="../../Content/styles/owl.carousel.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
<!-- secțiunea antetului -->
   <div class="header_section">
       <div class="pos-f-t">
           <div class="collapse" id="navbarToggleExternalContent">
               <div class="bg-dark p-4">
                   <div class="menu_main">
                       <ul>
                           <li>@Html.ActionLink("Acasă", "Index", "Home")</li>
                           <li>@Html.ActionLink("Despre", "About", "Home")</li>
                           <li>@Html.ActionLink("Servicii", "Services", "Home")</li>
                           <li>@Html.ActionLink("Clienți", "Clients", "Home")</li>
                           <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                       </ul>
                   </div>
               </div>
           </div>
           <nav class="navbar navbar-dark bg-dark">
               <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
                   <span class="navbar-toggler-icon"></span>
               </button>
                <div><a href="index.html"><img src="../../Content/images/logo.png"></a></div>
                           <div><a href="index.html"><img src="../../Content/images/search-icon.png"></a></div>
           </nav>
       </div>
   </div>

   <!-- sfârșitul secțiunii antetului -->
   <div class="vehicle_section">
      <div class="container">
         <h1 class="vehicle_taital">Servicii de Reparații Auto</h1>
         <p class="long_text">Este un fapt dovedit că un cititor va fi distras de</p>
      </div>
   </div>
   <!-- secțiunea bannerului -->
   <div class="banner_section">
      <div id="main_slider" class="carousel slide" data-ride="carousel">
         <div class="carousel-inner">
            <div class="carousel-item active">
               <div class="container">
                  <div class="image_1"><img src="../../Content/images/img-1.png"></div>
               </div>
            </div>
            <div class="carousel-item">
               <div class="container">
                  <div class="image_1"><img src="../../Content/images/img-1.png"></div>
               </div>
            </div>
            <div class="carousel-item">
               <div class="container">
                  <div class="image_1"><img src="../../Content/images/img-1.png"></div>
               </div>
            </div>
         </div>
         <a class="carousel-control-prev" href="#main_slider" role="button" data-slide="prev">
            <i class="fa fa-angle-left"></i>
         </a>
         <a class="carousel-control-next" href="#main_slider" role="button" data-slide="next">
            <i class="fa fa-angle-right"></i>
         </a>
      </div>
   </div>
   <!-- sfârșitul secțiunii bannerului -->
       @RenderBody()
    <!-- Începutul secțiunii de subsol -->
         <div class="footer_section layout_padding">
            <div class="container">
               <div class="row">
                  <div class="col-lg-3 col-sm-6">
                     <h4 class="footer_taital">INFORMAȚII DE CONTACT</h4>
                     <div class="call_text"><img src="../../Content/images/call-icon.png"><span class="padding_left_10">+01 1234567890</span></div>
                     <div class="call_text"><img src="../../Content/images/mail-icon.png"><span class="padding_left_10">demo@gmail.com</span></div>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                     <h4 class="footer_taital">PROGRAM DE LUCRU</h4>
                     <div class="call_text">Luni-Sâmbătă  07:00 - 05:00</div>
                     <div class="call_text">Duminică-Sâmbătă  07:00 - 05:00</div>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                     <h4 class="footer_taital">media socială</h4>
                     <div class="social_icon">
                        <ul>
                           <li><a href="#"><img src="../../Content/images/fb-icon.png"></a></li>
                           <li><a href="#"><img src="../../Content/images/twitter-icon.png"></a></li>
                           <li><a href="#"><img src="../../Content/images/linkedin-icon.png"></a></li>
                           <li><a href="#"><img src="../../Content/images/instagram-icon.png"></a></li>
                        </ul>
                     </div>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                     <h4 class="footer_taital">Newsletter</h4>
                     <input type="text" class="enter_text" placeholder="Introduceți adresa dvs. de email" name="Introduceți adresa dvs. de email">
                     <div class="subscribe_bt"><a href="#">Abonează-te</a></div>
                  </div>
               </div>
               <div class="copyright_section">
                  <h1 class="copyright_text">Copyright 2019 Toate drepturile rezervate <a href="https://html.design">Template-uri Html Gratuite</a>
                  </h1>
               </div>
            </div>
         </div>
         <!-- Sfârșitul secțiunii de subsol -->

<script src="../../Scripts/jquery.min.js"></script>
      <script src="../../Scripts/popper.min.js"></script>
      <script src="../../Scripts/bootstrap.bundle.min.js"></script>
      <script src="../../Scripts/jquery-3.0.0.min.js"></script>
      <script src="../../Scripts/plugin.js"></script>
      <!-- sidebar -->
      <script src="../../Scripts/jquery.mCustomScrollbar.concat.min.js"></script>
      <script src="../../Scripts/custom.js"></script>
      <!-- javascript --> 
      <script src="../../Scripts/owl.carousel.js"></script>
      <script src="https:cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.js"></script> 
      <script type="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2//2.0.0-beta.2.4/owl.carousel.min.js"></script>
      <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
      <script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery-slim.min.js"><\/script>')</script>
      <script src="../../Scripts/popper.min.js"></script>
      <script src="../../Scripts/bootstrap.min.js"></script>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</body>
</html>